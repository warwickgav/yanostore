}
#about-us {
#velaBreadcrumbs {
@include bg-breadcrumb("{{ 'bg_about.jpg' | asset_url }}",{{ settings.bg_about_parallax }});
}
}
#contac-us {
#velaBreadcrumbs {
@include bg-breadcrumb("{{ 'bg_contact.jpg' | asset_url }}",{{ settings.bg_contact_parallax }});
}
}
