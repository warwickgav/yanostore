//========== VELA - VARIABLES  ==========//
$vela-color-primary:            {{ settings.vela_color_primary }};
$vela-color-secondary:          {{ settings.vela_color_secondary }};
$vela-color-tertiary:           {{ settings.vela_color_tertiary }};
$vela-border-color:             {{ settings.vela_color_borders }};
$vela-body-bg:                  {{ settings.vela_color_background }};

$vela-text-color-primary:       {{ settings.text_color_primary }};
$vela-text-color-secondary:     {{ settings.text_color_secondary }};
$vela-text-color-tertiary:     {{ settings.text_color_tertiary }};

$vela-border-main:              1px solid $vela-border-color;
$vela-gutter-width:             30px;

$vela-font-family:              {{ settings.vela_fontfamily }};
$vela-font-family-secondary:    {{ settings.vela_fontfamily_secondary }};
$vela-font-family-tertiary:     {{ settings.vela_fontfamily_tertiary }};
$vela-font-icon:                "FontAwesome";

$vela-font-size:                {{ settings.vela_fontsize }};
$nocolor:                       transparent;

$hero-block-title-color: {{ settings.hero_block_title_color }};
$hero-block-text-color: {{ settings.hero_block_text_color }};

//========== BOOTSTRAP - VARIABLES  ==========//
$gray-base:              #333;
$gray-darker:            lighten($gray-base, 13.5%); // #222
$gray-dark:              lighten($gray-base, 20%);   // #333
$gray:                   lighten($gray-base, 33.5%); // #555
$gray-light:             lighten($gray-base, 46.7%); // #777
$gray-lighter:           lighten($gray-base, 93.5%); // #eee

$brand-primary:         $vela-color-primary; // #337ab7
$brand-success:         #5cb85c;
$brand-info:            #5bc0de;
$brand-warning:         #f0ad4e;
$brand-danger:          #d9534f;

$body-bg:               $vela-body-bg;
$text-color:            $vela-text-color-primary;
$link-color:            $vela-text-color-secondary;
$link-hover-color:      $vela-color-primary;
$link-hover-decoration: inherit;

$font-family-sans-serif:  $vela-font-family , Helvetica, Arial, sans-serif;
$font-family-serif:       Georgia, "Times New Roman", Times, serif;
$font-family-monospace:   Menlo, Monaco, Consolas, "Courier New", monospace;
$font-family-base:        $font-family-sans-serif;

$font-size-base:          $vela-font-size;
$font-size-large:         ceil(($font-size-base * 1.25)); // ~18px
$font-size-small:         ceil(($font-size-base * 0.85)); // ~12px

$font-size-h1:            floor(($font-size-base * 2.6)); // ~36px
$font-size-h2:            floor(($font-size-base * 2.15)); // ~30px
$font-size-h3:            ceil(($font-size-base * 1.7)); // ~24px
$font-size-h4:            ceil(($font-size-base * 1.25)); // ~18px
$font-size-h5:            $font-size-base;
$font-size-h6:            ceil(($font-size-base * 0.85)); // ~12px

$line-height-base:        1.428571429;
$line-height-computed:    floor(($font-size-base * $line-height-base));

$headings-font-family:    inherit;
$headings-font-weight:    500;
$headings-line-height:    1.1;
$headings-color:          inherit;

$padding-base-vertical:     8px;
