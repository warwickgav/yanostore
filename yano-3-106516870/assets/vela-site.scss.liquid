background-color:{{ settings.vela_header_bgcolor }};
}
.velaLogo {
margin:0px;
padding:0px;
display: inline-block;
.velaLogoLink{
display: block;
max-width:{{ settings.logo_max_width }};
}
}
.headerContent{
position:relative;
}
.velaHeaderLeft {
@media (min-width: $screen-md-min) {
padding-top: 30px;
}
}
.velaHeaderCenter {
text-align: center;
}
.velaHeaderRight {
@media (min-width: $screen-md-min) {
padding-top: 27px;
}
}


//MAIN CONTENT
#pageContent{
position:relative;
overflow:hidden;
}
//FOOTER
.footerCenter{
position:relative;
background-color:{{ settings.vela_footercenter_bgcolor }};
.footerCenterWrap {
padding:40px 0px 10px;
border-top: 1px solid $vela-border-color;
border-bottom: 1px solid $vela-border-color;
}
}
.footerCopyRight{
position:relative;
padding:50px 0px;
text-align: center;
text-transform: uppercase;
color: #ccc;
background-color:#ffffff;
}
.footer-logo .logo {
display: block;
width: 50px;
}
.list {
margin: 0;
padding: 0;
list-style: none;
}
.footer-copyright {
text-align: center;
}
.footer-menu {
border-top: 1px solid #E6E4E4;
border-bottom: 1px solid #E6E4E4;
padding-top: 20px;
padding-bottom: 20px;
h4 {
font-size: 12px;
text-transform: uppercase;
letter-spacing: 1px;
}
a {
color: $vela-color-primary;
&:hover, &:focus, &:active {
color: $vela-color-secondary;
}
}
}
//========== VELA - TITLE BLOCK, PAGE ==========//
.velaBlock {
margin-bottom: $grid-gutter-width;
}
.velaTitle {
font-size: 18px;
text-transform: uppercase;
color: $vela-heading-color;
display: block;
position: relative;
padding: 10px 0;
font-weight: 700;
}
.velaHomeTitle{
padding:10px 0px;
text-align:center;
&:before {
position: absolute;
bottom: 0;
@include size(60px,2px);
background-color: $vela-color-primary;
left: 50%;
margin-left: -30px;
content: "";
}
}
.velaFooterTitle{
font-family:$vela-font-family;
font-size:$vela-font-size + 4;
font-weight:normal;
margin:0px;
padding:15px 0px;
}
.titleSidebar{
font-family:$vela-font-family;
font-size:$vela-font-size + 6;
font-weight:bold;
line-height:20px;
border-bottom:$vela-border-main;
margin: 0 0 10px;
padding:10px 0px;
position:relative;
text-transform: uppercase;
color: $vela-heading-color;
}
.velaArticleTitle{
font-family:$vela-font-family;
font-size:$vela-font-size + 12;
font-weight:normal;
margin:0px;
padding:12px 0px;
color: $vela-heading-color;
text-transform: uppercase;
}
.velaPageTitle{
font-family:$vela-font-family;
font-size:$vela-font-size + 4;
font-weight:normal;
color: $vela-heading-color;
margin:0px;
padding:15px 0px;
}
//========== VELA - SIDEBAR ==========//
.velaSidebar{
>div{
margin-top:$vela-gutter-width;
&:first-child{
margin-top:0px;
}
&:last-child{
margin-bottom:$vela-gutter-width;
}
}
}
//========== VELA - BLOCK STYLES ==========//
//Topbar Links
.velaTopLinks{
float:right;
ul{
margin:0px;
padding:0px;
li{
a{
display: block;
padding:10px;
color:$vela-text-color-secondary;
line-height:20px;
&:hover{
color:$vela-color-primary;
}
}
}
}
}
.topLinksTitle{
display:none;
padding:10px 15px;
line-height:20px;
color:$vela-text-color-secondary;
}
//Header Cart Top
.velaCartTop {
float:right;
a{
background: $vela-color-tertiary;
position:relative;
display:block;
color: #fff;
padding: 2px 10px;
&:hover,&:focus{
background: $vela-text-color-primary;
color: #fff;
text-decoration:none;
}
}
.fa, span {
display: inline-block;
vertical-align: middle;
}
.fa {
font-size: 16px;
margin-right: 5px;
}
}
//Header Search Top
#velaSearchIcon{
display:none;
}
#velaSearchTop{

}
.formSearch{
border-bottom: 1px solid $vela-border-color;
padding-right: 30px;
}
.velaSearch{
border:0px;
height:30px;
padding: 0;
@include box-shadow(none);

}
.btnVelaSearch{
position:absolute;
top:2px;
right:0;
line-height: 30px;
width:30px;
height: 30px;
color: $vela-text-color-secondary;
background: transparent;
border: 0;
padding: 0;
text-align: center;
&:hover {
color: $vela-color-primary;
}
.btnSearchText{
display:none;
}
}
.search-top {
float: left;
position: relative;
}
//Footer Links
.velaFooterLinks{
margin:0px;
padding:10px 0px;
li{
a{
padding:8px 0px;
@include inline-block;
color:$vela-text-color-primary;
&:hover,&:focus{
color:$vela-color-primary;
text-decoration:none;
}
}
}
}
// Hero
.hero {
min-height:20em;
margin-bottom: 20px;
background-repeat: no-repeat;
background-size: cover;
background-position: center;
position: relative;
@media (min-width: 700px) {
min-height: 30em;
}
@media (min-width: $screen-md) {
min-height: 35em;
}
@media (min-width: $screen-lg) {
min-height: 40em;
}
}
.hero__banner {
display: block;
position: absolute;
width: 100%;
height: 100%;
z-index: 1;
&:before {
content: '';
display: inline-block;
height: 100%;
vertical-align: middle;
}
}
.hero__content {
display: inline-block;
vertical-align: middle;
width: 100%;
z-index: 100;
text-align: center;
margin: 0 -3px;
padding: 0 20px;
}
.hero__title {
color: $hero-block-title-color;
margin: 0 0 20px;
font-size: 24px;
@media (min-width: 700px) {

}
@media (min-width: $screen-md) {

}
@media (min-width: $screen-lg) {

}
}
.hero__text {
border: 2px solid $hero-block-text-color;
color: $hero-block-text-color;
display: inline-block;
padding: 15px 25px;
text-transform: uppercase;
letter-spacing: 1px;
}

// $Image Block
.imageblock-feature {
display: block;
position: relative;
overflow: hidden;
&:before {
content: '';
display: inline-block;
height: 100%;
vertical-align: middle;
}
.overlay {
background-color: transparent;
background-color: rgba(0,0,0,0.2);
position: absolute;
z-index: 1;
width: 100%;
height: 100%;
}
figcaption {
display: inline-block;
vertical-align: middle;
position: relative;
width: 100%;
z-index: 100;
text-align: center;
margin: 0 -3px;
padding: 0 20px;
}
}
.imageblock__image {
display: block;
width: 100%;
max-width: 100%;
overflow: hidden;
}


//Home Produt Tabs
.proTabNav{
margin:0px;
padding:0px;
text-align: center;
>li{
display: inline-block;
position: relative;
&:before {
content: "/";
position: absolute;
left: -5px;
top: 10px;
color: $vela-text-color-primary;
}
&:first-child {
&:before {
display: none;
}
}
> a {
color: $vela-text-color-primary;
position: relative;
font-size: 18px;
&:before {
position: absolute;
@include size(60px,2px);
background-color: $nocolor;
left: 50%;
margin-left: -30px;
bottom: 0;
content: "";
}
&:hover,&:focus{
color:$vela-text-color-primary;
}
}
&.active{
> a{
color:$vela-color-secondary;
border-color:$nocolor;
&:before {
background-color: $vela-color-primary;
}
}
}
}
}
//Product Page Social Sharing
.socialSharing{
margin:0px;
padding:0px;
> li{
@include inline-block;
margin-right:4px;
.btnSharing{
display:block;
font-size:0px;
color:white;
&:hover{
@include box-shadow(0 2px 5px 0 rgba(0,0,0,0.16),0 2px 10px 0 rgba(0,0,0,0.12));
}
.fa{
font-size:$vela-font-size - 2;
width:26px;
height:26px;
line-height:26px;
text-align:center;
}
&.btnTwitter{
.fa{
background-color:#00acee;
}
}
&.btnFacebook{
.fa{
background-color:#3a5898;
}
}
&.btnGooglePlus{
.fa{
background-color:#de6463;
}
}
&.btnLinkedin{
.fa{
background-color:#0177b5;
}
}
&.btnPinterest{
.fa{
background-color:#c72527;
}
}
}
}
}
// Sidebar - Block Categories
.velaCategoriesSidebar{
.itemCategory{

}
.panel-body{
padding:0px 15px;
}
.cateTitle{
margin:0px;
padding:0px;
>a{
position:relative;
display: block;
font-size:$vela-font-size;
font-weight:normal;
color: $vela-heading-color;
padding:10px 0px;
text-decoration:none;
&:before{
content:"";
@include size(12px,12px);
border: $vela-border-main;
display: inline-block;
margin-right: 7px;
font-family: $vela-font-icon;
}
&.hasSubCategory.collapsed{
&:before{
content:"\f107";
}
}
&.hasSubCategory{
&:before{
content:"\f106";
}
}
&.active,
&:hover{
color:$vela-color-primary;
&:before {
content: "\f046";
border: 0;
}
}
}
}
.listSidebar{
margin:0px;
padding:0px;
li{
border-top:1px dashed $vela-border-color;
a{
display:block;
font-size:$vela-font-size;
font-weight:normal;
color:$vela-text-color-secondary;
padding:10px 0px;
text-decoration:none;
&:hover{
color:$vela-color-primary;
}
}
}
}
}
// Sidebar - List Filter
.listSidebar{
margin:0px;
padding:12px 0px;
li{
a{
display:block;
font-size:$vela-font-size;
font-weight:normal;
color:$vela-text-color-secondary;
padding:5px 0px;
text-decoration:none;
&:hover{
color:$vela-color-primary;
}
}
}
}
// Sidebar - List Filter
.ajaxFilter{
margin:0px;
padding:10px 0px;
.ajaxFilterTitle{
position:relative;
font-family: $vela-font-family;
font-size: $vela-font-size;
font-weight: bold;
margin: 0px 0 15px;
padding: 0 0px 10px;
&:before{
content: "";
position:absolute;
bottom: 0;
left: 0;
@include size(20px,2px);
background-color: $vela-text-color-secondary;
}
}
}
.listFilter{
margin:0px;
padding:0px;
> li{
a{
position:relative;
display:block;
font-size:$vela-font-size;
font-weight:normal;
padding:5px 0px;
color:$vela-text-color-secondary;
text-decoration:none;
&:before{
content:"";
@include size(12px,12px);
border: $vela-border-main;
display: inline-block;
margin-right: 7px;
font-family: $vela-font-icon;
}
&:hover{
color:$vela-color-primary;
&:before{
content:"\f046";
border: 0;
}
}
}
&.active{
a{
color:$vela-color-primary;
&:before{
content:"\f046";
border: 0;
}
}
}
}
&.listFilterColor{
> li{
margin-bottom: 10px;
a{
padding:0px;
span {
margin-left: 10px;
}
img {
@include border-radius(50%);
@include size(15px,15px);
float: left;
margin-top: 4px;
padding: 2px;
border: $vela-border-main;
}
&:before{display:none;}
}
&:hover,
&.active{
a {
border-color:$vela-color-primary;
img {
border-color: $vela-color-primary;
}
}
}
}
}
}
.velaFilter{
.velaClearAll{
font-size:$vela-font-size - 1;
font-weight:300;
font-style:italic;
color:$vela-text-color-secondary;
position:absolute;
top:15px;
right:0px;
z-index:1;
}
.velaClear{
font-size:$vela-font-size - 2;
font-weight:300;
font-style:italic;
color:$vela-text-color-secondary;
position:absolute;
top:10px;
right:0px;
z-index:1;
}
}
// Sidebar - List Article in Blog
.listSidebarBlog{
margin:0px;
padding:0px;
li {
padding-top: $vela-gutter-width/2;
padding-bottom: $vela-gutter-width/2;
.titleBlogsidebar {
display:block;
font-size:$vela-font-size;
font-weight: normal;
color: $vela-text-color-secondary;
text-decoration:none;
text-transform: uppercase;
margin-bottom: 5px;
max-height: 40px;
overflow: hidden;
line-height: 20px;
&:hover{
color:$vela-color-primary;
}
time{
display:block;
font-size:$vela-font-size - 1;
font-weight: 300;
font-style:italic;
color: $vela-text-color-tertiary;
}
}
}
}
// Sidebar - List Tag in Blog
.blogTagsList{
margin:0px;
padding:15px 0px;
li{
margin:0px 10px 10px 0px;
padding:0px;
@include inline-block;
a{
background-color:#ebebeb;
color: $vela-heading-color;
display:block;
padding: 4px 8px;
&:hover,&:focus{
background-color:$vela-color-primary;
color:white;
text-decoration:none;
}
}
}
}
// Contact Page - Time Work
.timeWork{
margin:0px;
padding:0px;
list-style:none;
@include clearfix;
li{
position:relative;
float:left;
width:100%;
padding:5px 0px;
&:before{
content:"";
position:absolute;
top:50%;
left:0px;
right:0px;
border-top:1px dotted $vela-border-color;
z-index:-1;
}
span{
background-color:$vela-body-bg;
}
}
}
// AJAX CART - MODAL
.rowAjaxCart{
margin-left:-10px;
margin-right:-10px;
}
.rowAjaxCart > [class*='col-'] {
padding-right:10px;
padding-left:10px;
}
.velaCartTitle{
position:relative;
font-family:$vela-font-family;
font-size:$vela-font-size + 10;
margin:0px;
padding:15px 0px;
border-bottom:$vela-border-main;
color: $vela-heading-color;
.close{
position:absolute;
top:22px;
right:10px;
}
}
.ajaxCartModal{
.modal-content{
@include border-radius(0px);
padding:20px 30px;
}
}
#cartContainer{
padding:10px 0px 10px;
}
.ajaxcartQty{
position:relative;
@include clearfix;
.qtyAdjust{
float:left;
font-size:20px;
width:36px;
height:36px;
border:$vela-border-main;
background-color:white;
color:$vela-text-color-secondary;
&.qtyMinus{
border-right:0px;
}
&.qtyPlus{
border-left:0px;
}
}
.qtyNum{
float:left;
height:36px;
width:60px;
border:$vela-border-main;
background-color:white;
color:$vela-text-color-primary;
padding:6px 12px;
text-align:center;
}
}
.cartSubtotal{
font-size:$vela-font-size + 10;
font-weight:bold;
margin:0px 0px 0px 10px;
padding: 5px 0px;
color: $vela-text-color-secondary;
font-family: $vela-font-family-secondary;
}
.ajaxcartProduct{
padding:10px 0px;
border-bottom:$vela-border-main;
.ajaxcartQty{
@include inline-block;
}
.ajaxcartProductName{
font-family:$vela-font-family;
font-size:$vela-font-size + 1;
display:block;
margin:0px 0px 10px;
color: $vela-text-color-secondary;
&:hover,&:focus{
color:$vela-color-primary;
text-decoration:none;
}
}
.ajaxcartProductMeta{
font-style:italic;
}
.ajaxcartProductImage{
display:block;
}
@media (min-width:992px){
.textRight{
text-align:right;
}
}
}
.additional_checkout_buttons{
padding: 10px 0px;
}
.ajaxNote{
label{
display:block;
font-weight:normal;
}
}
.ajaxcartHeader{
margin:0px;
border-top:$vela-border-main;
border-bottom:$vela-border-main;
background-color:#fbfbfb;
span{
display:block;
padding: 10px 0px;
font-weight:500;
}
}
.ajaxCartFooter{
margin-top:30px;
}
@media (max-width:768px) {
.ajaxcartProduct{
.ajaxcartProductImage{
margin-bottom:15px;
}
.ajaxProductInfo{
margin-bottom:15px;
}
.priceProduct{
margin:15px 0px;
}
}
.ajaxCartFooter{
.ajaxSubTotal{
margin-top:15px;
}
}
}
.drawerRight{
.ajaxcartHeader{
display:none;
}
.drawerImage{
width:30% !important;
}
.drawerProRight{
float:right !important;
width:70% !important;
margin-bottom:8px;
&:last-child{
margin-bottom:0px;
}
.textRight{
text-align:left;
}
}
.drawerAjaxFooter{
width:100% !important;
margin-bottom:10px;
}
.ajaxButton{
.btn{width:100%;}
}
}
// NEWSLETTER - Vela Framework
#newsletterAlert{
.modal-content{
padding:3px;
}
.alert{margin:0px;}
}
.footer-newsletter.footer-newsletter {
background-color: $vela-color-primary;
padding: 20px 0;
@media (min-width: $screen-md-min) {
padding: 40px 0;
}
}
#vela-newsletter {
text-align: center;
margin-top: 10px;
.formNewsletter {
@media(min-width: $screen-xs-max) {
display: inline-block;
}
text-align: left;
.input-group {
@media(min-width: $screen-xs-max) {
min-width: 420px;
}
.form-control {
background-color: #fff;
border-color: #fff;
}
.btnNewsletter .btn {
background-color: transparent;
border-color: #fff;
color: #fff;
&:hover {
background-color: #333;
}
}
}
}
}

//Social link on Footer
@media(min-width: $screen-xs-max) {
#velaSocial {
text-align: right;
margin-top: 20px;
margin-bottom: 20px;
}
}
.velalistSocial {
text-align: center;
font-size: 26px;
.btn-social {
color: $vela-text-color-primary;
&:hover {
color: $vela-color-primary;
}
span {
display: none;
}
}
.list-inline > li {
padding-left: 20px;
padding-right: 20px;
}
}


